{
    "manifest_version": 3,
    "name": "Global Entry Drops",
    "description": "Find global entry interviews that meet your selection criteria.",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "alarms",
        "notifications"
    ]
}